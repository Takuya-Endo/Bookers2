<%= render 'shared/user-info-and-new-book', user: @user, book: @book %>
<!--↑部分テンプレート呼び出し-->

<% @users.each do |user| %>
  <p>
    <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpg' 'png', fallback: "no_image.jpg" %>
    <%= user.name %>
    <%= link_to "Show", user_path(user.id)%>
  </p>
<% end %>